    "selenia-subsystems/event-bus": "self.version",
    "selenia-subsystems/file-server": "self.version",
    "selenia-subsystems/http": "self.version",
    "selenia-subsystems/http-middleware": "self.version",
    "selenia-subsystems/interfaces": "self.version",
    "selenia-subsystems/localization": "self.version",
    "selenia-subsystems/logging": "self.version",
    "selenia-subsystems/mail": "self.version",
    "selenia-subsystems/media": "self.version",
    "selenia-subsystems/routing": "self.version",
    "selenia-subsystems/sessions": "self.version",
    "selenia-subsystems/tasks": "self.version",
    "selenia-subsystems/validation": "self.version",
    "selenia-subsystems/view-engine": "self.version",
    "selenia-subsystems/web-server": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Selenia\\": "Selenia"
